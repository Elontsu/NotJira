{
	"name": "notjira",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"format": "biome format --write ./src",
		"lint": "biome lint --write ./src",
		"preview": "vite preview",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{js,ts,jsx,tsx,json,css}": [
			"biome check --write --no-errors-on-unmatched"
		]
	},
	"dependencies": {
		"@reduxjs/toolkit": "^2.11.1",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-redux": "^9.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.5",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.1",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"msw": "^2.12.4",
		"typescript": "~5.9.3",
		"vite": "^7.2.4"
	}
}
